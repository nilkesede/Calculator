{
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "rnstl",
    "pre-push": "npm run lint && npm test"
  },
  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.62.2",
    "styled-components": "5.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.10.2",
    "@babel/runtime": "7.10.2",
    "@react-native-community/eslint-config": "1.1.0",
    "@storybook/addon-actions": "5.3.19",
    "@storybook/addon-knobs": "5.3.19",
    "@storybook/addon-ondevice-actions": "5.3.19",
    "@storybook/addon-ondevice-knobs": "5.3.19",
    "@storybook/addons": "5.3.19",
    "@storybook/react-native": "5.3.19",
    "@storybook/react-native-server": "5.3.19",
    "@types/jest": "25.2.3",
    "@types/react-native": "0.62.12",
    "@types/react-test-renderer": "16.9.2",
    "@types/styled-components": "5.1.0",
    "@typescript-eslint/eslint-plugin": "3.1.0",
    "@typescript-eslint/parser": "3.1.0",
    "babel-jest": "26.0.1",
    "babel-loader": "8.1.0",
    "eslint": "7.1.0",
    "eslint-config-xo-space": "0.25.0",
    "eslint-plugin-unicorn": "20.1.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "metro-react-native-babel-preset": "0.59.0",
    "prettier": "2.0.5",
    "react-dom": "16.13.1",
    "react-native-storybook-loader": "1.8.1",
    "react-test-renderer": "16.13.1",
    "typescript": "3.9.3"
  },
  "private": true,
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook/stories"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/story-loader.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run pre-push"
    }
  }
}
